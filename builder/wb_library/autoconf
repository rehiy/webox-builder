### Update: 2012-04-25 ###

export wk_autoconf=autoconf-2.69

wb_find_autoconf() {

    wb_apply http://mirrors.kernel.org/gnu/autoconf/ ${wk_autoconf} .tar.xz

}

wb_make_autoconf() {

    ./configure --prefix=${webox}

    make && make install

}

wb_inst_autoconf() {

    local fine=${webox}/bin/autoconf

    [ -e ${fine} ] || wb_find_autoconf
    [ -e ${fine} ] || wb_make_autoconf
    [ -e ${fine} ] || wb_error ${wk_autoconf}

}
