### Update: 2016-112-31 ###

export wk_m4=m4-1.4.18

wb_make_m4() {

    wb_apply http://mirrors.kernel.org/gnu/m4/ ${wk_m4} .tar.xz

    ./configure --prefix=${webox}

    make && make install

}

wb_inst_m4() {

    local fine=${webox}/bin/m4

    [ -e ${fine} ] || wb_make_m4
    [ -e ${fine} ] || wb_error ${wk_m4}

}
