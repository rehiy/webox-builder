wk_jpeg=jpeg-9a

wb_make_jpeg() {

    wb_apply http://www.ijg.org/files/ jpegsrc.v${1##*-} .tar.gz ${1}

    ./configure --prefix=${webox}

    make && make install

}

wb_inst_jpeg() {

    local name=${wk_jpeg}
    local fine=${webox}/lib/libjpeg.a

    [ -e ${fine} ] || wb_make_jpeg ${name}
    [ -e ${fine} ] || wb_error ${name}

}
