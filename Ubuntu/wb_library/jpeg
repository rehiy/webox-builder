export wk_jpeg=jpeg-9a

wb_make_jpeg() {

    wb_apply http://www.ijg.org/files/ jpegsrc.v${wk_jpeg##*-} .tar.gz ${wk_jpeg}

    ./configure --prefix=${webox}

    make && make install

}

wb_inst_jpeg() {

    local fine=${webox}/lib/libjpeg.a

    [ -e ${fine} ] || wb_make_jpeg
    [ -e ${fine} ] || wb_error ${wk_jpeg}

}
