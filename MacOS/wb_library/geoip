export wk_geoip=GeoIP-1.4.8

wb_make_geoip() {

    wb_apply http://geolite.maxmind.com/download/geoip/api/c/ ${wk_geoip} .tar.gz

    ./configure --prefix=${webox}

    make && make install

}

wb_inst_geoip() {

    local fine=${webox}/lib/libGeoIP.a

    [ -e ${fine} ] || wb_make_geoip
    [ -e ${fine} ] || wb_error ${wk_geoip}

}
